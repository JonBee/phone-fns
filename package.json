{
  "name": "phone-fns",
  "version": "3.0.0",
  "description": "A small, modern, and functional phone library for javascript",
  "main": "dist/phone-fns.min.js",
  "module": "src/index.js",
  "scripts": {
    "prepack": "npm run scripts && rollup -c",
    "scripts": "node scripts/docs.js && node scripts/create-export.js",
    "build": "rollup -c",
    "postpublish": "node scripts/clean.js",
    "lint": "standard src/**/*.js",
    "test": "tape -r esm tests/*.js | tap-spec",
    "test:cov": "nyc npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/index.js",
      "tests/**"
    ]
  },
  "standard": {
    "ignore": [
      "docs.js",
      "dist/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "homepage": "https://dusty.codes/documentation/phone-fns",
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone libary",
    "phone",
    "phone-fns",
    "functional library",
    "formatting",
    "modern"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "coveralls": "3.0.2",
    "esm": "3.0.84",
    "globby": "8.0.1",
    "jsdoc-to-markdown": "4.0.1",
    "nyc": "13.0.1",
    "rollup": "0.66.6",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-cleanup": "3.0.0",
    "rollup-plugin-filesize": "4.0.1",
    "rollup-plugin-terser": "3.0.0",
    "standard": "12.0.1",
    "tap-spec": "5.0.0",
    "tape": "4.9.1"
  },
  "dependencies": {
    "kyanite": "0.9.1"
  }
}
