{
  "name": "phone-fns",
  "version": "0.2.0",
  "description": "Small and simple functional phone library",
  "main": "phone-fns.min.js",
  "scripts": {
    "compile:breakdown": "rollup -i src/breakdown/index.js -o breakdown/index.js -f umd -n breakdown",
    "compile:find": "rollup -i src/find/index.js -o find/index.js -f umd -n find",
    "compile:format": "rollup -i src/format/index.js -o format/index.js -f umd -n format",
    "compile:identical": "rollup -i src/identical/index.js -o identical/index.js -f umd -n identical",
    "compile:isValid": "rollup -i src/isValid/index.js -o isValid/index.js -f umd -n isValid",
    "compile:uglify": "rollup -i src/uglify/index.js -o uglify/index.js -f umd -n uglify",
    "compile:findLocal": "rollup -i src/findLocal/index.js -o findLocal/index.js -f umd -n findLocal",
    "compile:getCode": "rollup -i src/getCode/index.js -o getCode/index.js -f umd -n getCode",
    "compile:callingCodes": "rollup -i src/callingCodes.js -o callingCodes.js -f umd -n getCode",
    "build:all": "npm-run-all --parallel compile:*",
    "cleanup": "rimraf {esm,breakdown,find,format,identical,isValid,uglify,findLocal,getCode}",
    "prepare": "rollup -c && rollup -c rollup.minify.js && npm run build:all",
    "postpublish": "npm run cleanup",
    "lint": "eslint ./src",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "env",
      "@ava/stage-4",
      "@ava/transform-test-files"
    ]
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone",
    "phone-fns",
    "functional library",
    "formatting"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "ava": "^0.23.0",
    "babel": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.8.0",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "rollup": "0.50.0",
    "rollup-plugin-buble": "0.16.0",
    "rollup-plugin-uglify": "2.0.1",
    "tap-spec": "4.1.1"
  }
}
