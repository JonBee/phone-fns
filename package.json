{
  "name": "phone-fns",
  "version": "0.3.1",
  "description": "Small and simple functional phone library",
  "main": "./index.js",
  "scripts": {
    "build:all": "npm-run-all --parallel compile:*",
    "cleanup": "rimraf {esm,breakdown,find,format,identical,isValid,uglify,findLocal,getCode,getCountries,getCountryCode,main,callingCodes}",
    "prepare": "webpack",
    "postpublish": "npm run cleanup",
    "lint": "eslint ./src",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "env",
      "@ava/stage-4",
      "@ava/transform-test-files"
    ]
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone",
    "phone-fns",
    "functional library",
    "formatting"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "ava": "0.24.0",
    "babel": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "buble-loader": "0.4.1",
    "eslint": "4.14.0",
    "globby": "7.1.1",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "webpack": "3.10.0"
  }
}
