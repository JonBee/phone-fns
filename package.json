{
  "name": "phone-fns",
  "version": "0.1.0",
  "description": "Silly simple phone functional library",
  "main": "src/index.js",
  "module": "src/esm/index.js",
  "scripts": {
    "prepare": "rollup -c",
    "lint": "eslint ./src",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "env",
      "@ava/stage-4",
      "@ava/transform-test-files"
    ]
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone",
    "phone-fns",
    "functional library",
    "formatting"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "ava": "^0.23.0",
    "babel": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.8.0",
    "rollup": "0.50.0",
    "rollup-plugin-buble": "0.16.0",
    "rollup-plugin-uglify": "2.0.1",
    "tap-spec": "4.1.1"
  }
}
