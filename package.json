{
  "name": "phone-fns",
  "version": "1.0.0",
  "description": "Silly simple phone functional library",
  "main": "dist/phone-fns.umd.js",
  "scripts": {
    "prepare": "rollup -c",
    "rollup:tests": "rollup -i src/tests/tests.js -f cjs -o src/tests/tests.cjs.js -e tape,moment && rollup -c",
    "pretest": "npm run rollup:tests",
    "lint": "eslint ./src",
    "build:tests": "rollup --config rollup.config.test.js",
    "test": "npm run build:tests && tape src/tests/tests.cjs.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone",
    "phone-fns",
    "functional library",
    "formatting"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "eslint": "^4.8.0",
    "rollup": "0.50.0",
    "rollup-plugin-buble": "0.16.0",
    "rollup-plugin-uglify": "2.0.1",
    "tap-spec": "4.1.1",
    "tape": "4.8.0"
  },
  "dependencies": {}
}
