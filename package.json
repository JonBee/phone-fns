{
  "name": "phone-fns",
  "version": "2.0.0",
  "description": "Small and simple functional phone library",
  "main": "dist/phone-fns.min.js",
  "scripts": {
    "prepack": "rollup -c && rollup -c rollup.split.js && node scripts/docs.js",
    "clean": "node scripts/clean.js",
    "build": "rollup -c",
    "build:split": "rollup -c rollup.split.js",
    "postpublish": "node scripts/clean.js",
    "lint": "standard src/**/*.js",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "failFast": true,
    "concurrency": 5,
    "babel": {
      "testOptions": {
        "presets": [
          "@ava/transform-test-files",
          "@ava/stage-4"
        ]
      }
    }
  },
  "standard": {
    "ignore": [
      "docs.js",
      "dist/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone libary",
    "phone",
    "phone-fns",
    "functional library",
    "formatting"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-stage-0": "7.0.0-beta.46",
    "@babel/preset-stage-2": "7.0.0-beta.46",
    "@babel/register": "7.0.0-beta.46",
    "ava": "1.0.0-beta.4",
    "del": "3.0.0",
    "globby": "8.0.1",
    "jsdoc-to-markdown": "4.0.1",
    "rollup": "0.58.1",
    "rollup-plugin-babel": "4.0.0-beta.4",
    "rollup-plugin-cleanup": "2.0.1",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-uglify": "3.0.0",
    "standard": "11.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
