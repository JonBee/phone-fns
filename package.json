{
  "name": "phone-fns",
  "version": "2.0.0",
  "description": "Small and simple functional phone library",
  "main": "dist/phone-fns.min.js",
  "scripts": {
    "prepack": "rollup -c && rollup -c rollup.split.js && node scripts/docs.js",
    "clean": "node scripts/clean.js",
    "build": "rollup -c",
    "build:split": "rollup -c rollup.split.js",
    "postpublish": "node scripts/clean.js",
    "lint": "standard src/**/*.js",
    "test": "tape -r esm tests/*.js | tap-spec",
    "test:cov": "nyc npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "standard": {
    "ignore": [
      "docs.js",
      "dist/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone libary",
    "phone",
    "phone-fns",
    "functional library",
    "formatting"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-0": "7.0.0-beta.44",
    "@babel/preset-stage-2": "7.0.0-beta.44",
    "coveralls": "3.0.1",
    "del": "3.0.0",
    "esm": "3.0.37",
    "globby": "8.0.1",
    "jsdoc-to-markdown": "4.0.1",
    "nyc": "11.8.0",
    "rollup": "0.59.3",
    "rollup-plugin-babel": "4.0.0-beta.4",
    "rollup-plugin-cleanup": "3.0.0-beta.1",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-uglify": "4.0.0",
    "standard": "11.0.1",
    "tap-spec": "4.1.1",
    "tape": "4.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
