{
  "name": "phone-fns",
  "version": "1.0.0",
  "description": "Small and simple functional phone library",
  "main": "index.js",
  "scripts": {
    "prepack": "webpack && webpack --config webpack.dist.js && node scripts/docs.js",
    "clean": "node scripts/clean.js",
    "build": "webpack",
    "build:dist": "webpack --config webpack.dist.js",
    "postpublish": "node scripts/clean.js",
    "lint": "eslint ./src",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "env",
      "@ava/stage-4",
      "@ava/transform-test-files"
    ]
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "failFast": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-fns"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-fns/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "functional",
    "phone libary",
    "phone",
    "phone-fns",
    "functional library",
    "formatting"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "ava": "0.25.0",
    "babel": "6.23.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "del": "3.0.0",
    "eslint": "4.14.0",
    "globby": "8.0.1",
    "jsdoc-to-markdown": "4.0.1",
    "webpack": "4.0.1",
    "webpack-cli": "2.0.9"
  }
}
